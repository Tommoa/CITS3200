<!--pages/final/final.wxml-->
<view class="page">

  <view class="page_hd">
    <view class="{{isIPX ? 'title-IPX' : 'title'}}">Survey Complete
      <icon type="success" size="25px" style="padding-top: 10px"></icon>
    </view>
  </view>

  <view class="desc">
    This is sample text after survey complection. Thanks for your cooperation.
  </view>



  <view class="page__bd page__bd_spacing">
    <button bindtap="bindViewTapHome" class="weui-btn" type="primary">Return</button>
  </view>


  <view class="page__bd">
    <view class="{{isIPX ? 'weui-tabbar-IPX' : 'weui-tabbar'}}">
      <view class="weui-tabbar__item">
        <view style="position: relative;display:inline-block;">
          <image src="../../images/surveyA.svg" class="weui-tabbar__icon"></image>
        </view>

      </view>
      <view bindtap="bindViewTap" class="weui-tabbar__item weui-bar__item_on">
        <view style="position: relative;display:inline-block;">
          <image src="../../images/user.svg" class="weui-tabbar__icon"></image>
        </view>
      </view>
    </view>
  </view>
</view>